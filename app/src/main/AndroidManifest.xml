<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.martemev.puzzlers"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RawActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".bridge.CollectionsGenericsActivity"/>
        <activity android:name=".bridge.VarargGenericsActivity"/>
        <activity android:name=".broadcast.ReceiverActivity"/>
        <activity android:name=".ErrorActivity"/>
        <activity android:name=".BlinkActivity"/>
        <activity android:name=".map.ParcelActivity"/>

        <service android:name=".broadcast.BroadcastingService"
            android:exported="false"/>

    </application>

</manifest>